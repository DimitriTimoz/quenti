{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "@quenti/next#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "start": {
      "outputs": ["dist/**", "build/**"]
    },
    "lint": {},
    "dev": {
      "cache": false
    }
  },
  "globalDependencies": ["**/.env", "yarn.lock"]
}
